<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.Magasin_Controleur">
   <children>
      <Pane prefHeight="570.0" prefWidth="797.0">
         <children>
            <Button fx:id="btn_ajouter" layoutX="594.0" layoutY="308.0" mnemonicParsing="false" onAction="#ajoutPanier" text="Ajouter au panier" />
            <Button fx:id="btn_retour" layoutX="34.0" layoutY="484.0" mnemonicParsing="false" onAction="#choixRetour" prefHeight="25.0" prefWidth="61.0" text="Retour" />
            <Spinner fx:id="spinner_qte" layoutX="594.0" layoutY="254.0" prefHeight="25.0" prefWidth="108.0" />
            <Label layoutX="34.0" layoutY="36.0" prefHeight="30.0" prefWidth="179.0" text="Liste de produit :" textFill="#ee0000">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TableView fx:id="TableView_produit" layoutX="34.0" layoutY="76.0" prefHeight="333.0" prefWidth="545.0" />
            <Button fx:id="btn_panier" layoutX="624.0" layoutY="54.0" mnemonicParsing="false" onAction="#afficherPanier" text="Panier" />
         </children></Pane>
   </children>
</AnchorPane>
